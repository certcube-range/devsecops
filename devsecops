pipeline {
  agent any
  tools {
    maven 'maven'
    }
    stages {
    stage ('initialize') {
      steps {
        sh '''
                echo  "PATH = ${PATH}"
                echo  "M2_home = ${M2_HOME}"
           '''     
        }
      }
      
     stage ('Build') {
        steps {
        sh 'mvn clean package'
        }
       }
       
  }
  
}
